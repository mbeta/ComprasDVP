<!DOCTYPE html>
<html lang="en">
    <head>
        {% block head %}            
            <meta charset="utf-8" />
            <title>Compras D.V.P.</title>


                <meta name="viewport" content="width=device-width, initial-scale=1.0, maximum-scale=1.0, user-scalable=no" />
                <meta name="apple-mobile-web-app-capable" content="yes" />    

            {% stylesheets '@AppComprasBundle/Resources/views/Template/css/base-admin-2-responsive.css' filter="cssrewrite" %}
                    <link href="{{ asset_url }}" rel="stylesheet" type="text/css" />
            {% endstylesheets %}
            {% stylesheets '@AppComprasBundle/Resources/views/Template/css/base-admin-2.css' filter="cssrewrite" %}
                    <link href="{{ asset_url }}" rel="stylesheet" type="text/css" />
            {% endstylesheets %}
            {% stylesheets '@AppComprasBundle/Resources/views/Template/css/custom.css' filter="cssrewrite" %}
                    <link href="{{ asset_url }}" rel="stylesheet" type="text/css" />
            {% endstylesheets %}
            {% stylesheets '@AppComprasBundle/Resources/views/Template/css/font-awesome.min.css' filter="cssrewrite" %}
                    <link href="{{ asset_url }}" rel="stylesheet" type="text/css" />
            {% endstylesheets %}
            {% stylesheets '@AppComprasBundle/Resources/views/Template/css/bootstrap-responsive.min.css' filter="cssrewrite" %}
                    <link href="{{ asset_url }}" rel="stylesheet" type="text/css" />
            {% endstylesheets %}
            {% stylesheets '@AppComprasBundle/Resources/views/Template/css/bootstrap.min.css' filter="cssrewrite" %}
                    <link href="{{ asset_url }}" rel="stylesheet" type="text/css" />
            {% endstylesheets %}


            <!-- Le HTML5 shim, for IE6-8 support of HTML5 elements -->
            <!--[if lt IE 9]>
              <script src="http://html5shim.googlecode.com/svn/trunk/html5.js"></script>
            <![endif]-->

            <meta http-equiv="Content-Type" content="text/html; charset=utf-8" />

        {% endblock %}
        
    </head>
        
    <body>
        
        {% block body %}   
            <div class="navbar navbar-inverse navbar-fixed-top">

                <div class="navbar-inner">

                    <div class="container">

                        <a class="btn btn-navbar" data-toggle="collapse" data-target=".nav-collapse">
                            <i class="icon-cog"></i>
                        </a>

                        <a class="brand" href="./template">
                            <img src="https://190.183.255.83/resolucionesV2/public/img/logo-head.png" width="180" height="50" alt>
                            "
                            Sistema Compras <sup>1.0</sup>
                            "

                        </a>		
                        	
                    </div> <!-- /container -->

                </div> <!-- /navbar-inner -->

            </div> <!-- /navbar -->





            <div class="subnavbar">

                <div class="subnavbar-inner">

                    <div class="container">                                        

                        <a class="btn-subnavbar collapsed" data-toggle="collapse" data-target=".subnav-collapse">
                            <i class="icon-reorder"></i>
                        </a>

                        <div class="subnav-collapse collapse">
                            <ul class="mainnav">

                                <li class="active">
                                    <a href="./template">
                                        <i class="icon-home"></i>
                                        <span>Home</span>
                                    </a>	    				
                                </li>

                                <li class="dropdown">					
                                    <a href="javascript:;" class="dropdown-toggle" data-toggle="dropdown">
                                        <i class="icon-th"></i>
                                        <span>Ubicaciones</span>
                                        <b class="caret"></b>
                                    </a>	    

                                    <ul class="dropdown-menu">
                                        <li><a href="http://localhost/ComprasDVP/web/app_dev.php/admin/app/compras/ubicacion/create">Nueva Ubicación</a></li>
                                        <li><a href="http://localhost/ComprasDVP/web/app_dev.php/admin/app/compras/ubicacion/list">Listar</a></li>                                    
                                    </ul> 				
                                </li>


                            </ul>
                        </div> <!-- /.subnav-collapse -->

                    </div> <!-- /container -->

                </div> <!-- /subnavbar-inner -->

            </div> <!-- /subnavbar -->


            <div class="main">

                {% block main %}

                {% endblock %}

            </div> <!-- /main -->


            <div class="footer">
                
                {% block footer %}    
                    <div class="container">

                        <div class="row">

                            <div id="footer-copyright" class="span6">
                                 D.V.P. - Departamento de Informática
                            </div> <!-- /span6 -->

                            <div id="footer-terms" class="span6">
                                <a href="http://www.vialidadchaco.net/home/" target="_blank">Vialidad Provincial Chaco</a>
                            </div> <!-- /.span6 -->

                        </div> <!-- /row -->

                    </div> <!-- /container -->
                {% endblock %}
            
            </div> <!-- /footer -->
            <!-- Le javascript
            ================================================== -->
            <!-- Placed at the end of the document so the pages load faster -->

            {% javascripts 
                '@js_template' %}
                <script type="text/javascript" src="{{ asset_url }}"></script>
            {% endjavascripts %}
        {% endblock %}
        
    </body>
</html>
